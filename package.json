{
  "name": "angular-material-electron",
  "version": "4.0.0",
  "description": "Angular + Material + Electron (Typescript, SASS, Hot Reload & Builder)",
  "homepage": "https://github.com/1nv1n/angular-material-electron.git",
  "author": {
    "name": "1nv1n"
  },
  "keywords": [
    "angular",
    "angular 6",
    "angular material",
    "electron",
    "electron builder",
    "material",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve --port 1337",
    "ng:serve:web": "npm run postinstall:web && ng serve -o --port 1337",
    "electron:tsc": "tsc main.ts",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:1337/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "lint": "tslint -c ./tslint.json \"./src/**/*.ts\" -e \"./src/**/typings.d.ts\" -e \"./src/environments/**\""
  },
  "dependencies": {
    "@angular/animations": "^9.1.6",
    "@angular/cdk": "^9.2.3",
    "@angular/flex-layout": "^9.0.0-beta.30",
    "@angular/http": "^7.2.16",
    "@angular/material": "^9.2.3",
    "hammerjs": "2.0.8",
    "rxjs-compat": "^6.5.5",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.5",
    "@angular/cli": "9.1.5",
    "@angular/common": "^9.1.6",
    "@angular/compiler": "^9.1.6",
    "@angular/compiler-cli": "6.1.7",
    "@angular/core": "^9.1.6",
    "@angular/forms": "^9.1.6",
    "@angular/language-service": "6.1.7",
    "@angular/platform-browser": "^9.1.6",
    "@angular/platform-browser-dynamic": "^9.1.6",
    "@angular/router": "^9.1.6",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/es6-collections": "0.5.31",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "core-js": "^3.6.5",
    "electron": "2.0.9",
    "electron-builder": "20.28.4",
    "electron-reload": "1.2.5",
    "jasmine-core": "3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "5.4.1",
    "rxjs": "^6.5.5",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "2.7.2",
    "wait-on": "3.0.1",
    "webdriver-manager": "12.1.0",
    "zone.js": "^0.10.3"
  }
}
